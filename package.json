    "@mui/material": "^6.1.5",
    "@mui/x-data-grid": "^7.22.3",
    "@react-pdf-viewer/core": "^3.12.0",
    "axios": "^1.7.7",
    "formik": "^2.4.6",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.0.7",
    "modern-normalize": "^3.0.1",
    "pdfjs-dist": "^3.4.120",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "xlsx": "^0.18.5",
    "yup": "^1.4.0"
